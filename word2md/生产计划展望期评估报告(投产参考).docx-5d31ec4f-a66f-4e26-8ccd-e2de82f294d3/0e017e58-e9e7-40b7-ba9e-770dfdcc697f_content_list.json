        ],
        "table_footnote": [],
        "table_body": "<table><tr><td>周期（天）</td><td>≤8</td><td>9-10</td><td>11-12</td></tr><tr><td>数量（项）</td><td>142</td><td>15</td><td>1</td></tr><tr><td>占比</td><td>90%</td><td>9%</td><td>1%</td></tr></table>",
        "bbox": [
