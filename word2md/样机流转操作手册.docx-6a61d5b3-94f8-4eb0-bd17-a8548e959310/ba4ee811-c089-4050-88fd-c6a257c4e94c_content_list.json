    {
        "type": "text",
        "text": "2.2功能操作",
        "text_level": 1,
        "bbox": [
            194,
            564,
            324,
            583
        ],
        "page_idx": 1
    },
    {
        "type": "image",
        "img_path": "images/2bd07b4a68adeec17cf6e01b17912b6a615b3a25396bcb884d4fda6473c89c86.jpg",
        "image_caption": [],
        "image_footnote": [],
        "bbox": [
            336,
